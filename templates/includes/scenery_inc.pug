| {% load staticfiles %}
section#scenery
  .container
    .row
      .col-md
        .box.box-conainer#scenery-carousel
          .row
            ul.col-md.content
              li.item.active
              li.item
              li.item
          .row.center-md.middle-md.carousel-indicators
            .col-md-3
              .box
                .row
                  a.col-md-1.carousel-control.left(role="button", data-slide="prev")
                    span.fa.fa-chevron-left(aria-hidden=true)
                  ol.col-md-10
                    li.active(data-slide-to="0")
                    li(data-slide-to="1")
                    li(data-slide-to="2")
                  a.col-md-1.carousel-control.right(role="button", data-slide="next")
                    span.fa.fa-chevron-right(aria-hidden=true)